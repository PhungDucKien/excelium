{
  "name": "@excelium/testkit",
  "version": "1.0.0",
  "description": "Excelium test stuff",
  "repository": "https://github.com/PhungDucKien/excelium",
  "keywords": [
    "excelium"
  ],
  "license": "Apache-2.0",
  "private": false,
  "main": "build/index.js",
  "types": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
