    "webdriverio": "^5.18.7"
  },
  "peerDependencies": {
    "selenium-webdriver": "^4.0.0-alpha.5",
    "webdriverio": "^5.18.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/selenium-webdriver": "^4.0.6",
    "typescript": "3.7.5"
  }
}
